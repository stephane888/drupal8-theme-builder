						{{ page.content }}
					</div>
					{# /.layout-content #}
				</div>
			</div>

			{% if page.sidebar_second %}
			<aside class="layout-sidebar-second" role="complementary">
				{{ page.sidebar_second }}
			</aside> 
			{% endif %} {% if page.featured_bottom_first %} {{ page.featured_bottom_first }}
